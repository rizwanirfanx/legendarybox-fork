    "custom_liquid_egAAfi": {
      "type": "custom-liquid",
      "custom_css": [],
      "settings": {
        "custom_liquid": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n  <meta charset=\"UTF-8\" />\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\n  <title>Plan Selector</title>\n  <style>\n    body {\n      font-family: 'Merriweather', serif;\n      background-color: #f9f9f9;\n      margin: 0;\n      padding: 0;\n      box-sizing: border-box;\n    }\n\n    .container {\n      max-width: 1100px; /* a touch wider to breathe */\n      margin: 20px auto;\n      background-color: #103948;\n      padding: 20px;\n      border-radius: 10px;\n      box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n      text-align: center;\n    }\n\n    h2 {\n      font-size: 28px;\n      margin-bottom: 30px;\n      color: #fff;\n    }\n\n    /* ==== GRID LAYOUT (fixes 4-across on desktop) ==== */\n    .plan-group {\n      display: grid;\n      grid-template-columns: repeat(4, minmax(0, 1fr));\n      gap: 20px;\n    }\n    @media (max-width: 992px) {\n      .plan-group { grid-template-columns: repeat(2, minmax(0, 1fr)); }\n    }\n    @media (max-width: 520px) {\n      .plan-group { grid-template-columns: 1fr; }\n    }\n\n    .plan {\n      border-radius: 10px;\n      padding: 20px 20px 26px;\n      text-align: center;\n      background-color: #fff;\n      transition: transform 0.3s, box-shadow 0.3s;\n      position: relative;\n      display: flex;\n      flex-direction: column;\n      gap: 8px;\n      min-height: 360px;\n    }\n\n    .plan:hover {\n      transform: translateY(-5px);\n      box-shadow: 0 6px 10px rgba(0, 0, 0, 0.1);\n    }\n\n    .plan-title {\n      font-size: 20px;\n      font-weight: bold;\n      color: #e63946;\n      margin-bottom: 6px;\n    }\n    .plan-title span { display: block; }\n\n    .plan-detail {\n      font-size: 24px;\n      font-weight: bold;\n      color: #333;\n      margin-bottom: 4px;\n    }\n    .plan-detail span { font-size: 16px; color: #666; }\n\n    .plan-total {\n      font-size: 16px;\n      color: #666;\n      margin-bottom: 6px;\n    }\n\n    .cta-button {\n      margin-top: 8px;\n      padding: 12px 40px;\n      background-color: #e63946;\n      color: #fff;\n      text-decoration: none;\n      font-size: 18px;\n      font-weight: bold;\n      text-transform: uppercase;\n      letter-spacing: 1px;\n      border-radius: 50px;\n      border: none;\n      display: inline-block;\n      transition: background-color 0.3s ease, transform 0.2s ease, box-shadow 0.3s ease;\n      box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n    }\n    .cta-button:hover {\n      background-color: #c71f1f;\n      transform: translateY(-2px);\n      box-shadow: 0 6px 10px rgba(0, 0, 0, 0.15);\n    }\n\n    .bonus-scoop {\n      font-size: 14px;\n      color: #c71f1f;\n      font-style: italic;\n      margin-top: 2px;\n    }\n\n    .prepaid-gift-info {\n      margin-top: 20px;\n      padding: 10px 20px;\n      background-color: transparent;\n      border-radius: 10px;\n      display: flex;\n      flex-direction: column;\n      align-items: center;\n      font-size: 16px;\n      font-weight: bold;\n      color: #A3F20C;\n    }\n    .scoop-thumb { margin-top: 5px; width: 200px; height: auto; }\n    .gift-text { text-align: center; }\n\n    /* ===== Smaller Entries badge ===== */\n    .entries-badge {\n      margin-top: auto;          /* pins badge to the card bottom */\n      align-self: center;        /* centered */\n      display: inline-flex;\n      align-items: center;\n      gap: 8px;\n      padding: 5px 8px 5px 10px;\n      border: 2px solid #0e3a48;\n      border-radius: 8px;\n      background: #ffffff;\n      color: #0e3a48;\n      font-weight: 700;\n      letter-spacing: 0.04em;\n      box-shadow: 0 2px 6px rgba(16, 57, 72, 0.15);\n      text-transform: uppercase;\n      white-space: nowrap;\n    }\n    .entries-badge .entries-label { font-size: 11px; }\n    .entries-badge .entries-count { font-size: 13px; margin: 0 2px; }\n    .entries-badge .multiplier {\n      background: #0e3a48;\n      color: #fff;\n      padding: 4px 8px;\n      border-radius: 6px;\n      font-size: 12px;\n      line-height: 1;\n      font-weight: 700;\n    }\n  </style>\n</head>\n<body>\n  <div id=\"plan-selector\" class=\"container\">\n    <h2>Choose Your Subscription Plan</h2>\n\n    <div class=\"plan-group\">\n      <!-- Monthly: 42 entries, 1X -->\n      <div class=\"plan\">\n        <div class=\"plan-title\">Monthly<span>Plan</span></div>\n        <div class=\"plan-detail\">$42.99<span>/mo.</span></div>\n        <div class=\"plan-total\">+ Shipping</div>\n        <a href=\"https://legendarybox.com/products/monthly-subscription-box\" class=\"cta-button\">Join Now</a>\n        <div class=\"entries-badge\">\n          <span class=\"entries-label\">Earn</span>\n          <span class=\"entries-count\">42</span>\n          <span class=\"entries-label\">Entries</span>\n          <span class=\"multiplier\">1X</span>\n        </div>\n      </div>\n\n      <!-- 3-Month: 125 entries, 1X -->\n      <div class=\"plan\">\n        <div class=\"plan-title\">3-Month<span>Prepaid</span></div>\n        <div class=\"plan-detail\">$41.99<span>/box</span></div>\n        <div class=\"plan-total\">$125.97<span> Total + s/h</span></div>\n        <a href=\"https://legendarybox.com/products/3-month-subscription-plan\" class=\"cta-button\">Join Now</a>\n        <div class=\"bonus-scoop\">Bonus Coffee Scoop!</div>\n        <div class=\"entries-badge\">\n          <span class=\"entries-label\">Earn</span>\n          <span class=\"entries-count\">125</span>\n          <span class=\"entries-label\">Entries</span>\n          <span class=\"multiplier\">1X</span>\n        </div>\n      </div>\n\n      <!-- 6-Month: 1199 entries, 5X -->\n      <div class=\"plan\">\n        <div class=\"plan-title\">6-Month<span>Prepaid</span></div>\n        <div class=\"plan-detail\">$39.99<span>/box</span></div>\n        <div class=\"plan-total\">$239.94<span> Total + s/h</span></div>\n        <a href=\"https://legendarybox.com/products