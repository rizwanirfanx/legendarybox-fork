    "custom_liquid_BtKkQi": {
      "type": "custom-liquid",
      "name": "t:sections.custom-liquid.presets.name",
      "settings": {
        "custom_liquid": "<!-- C&L: Each Box Includes (Mobile Text First) -->\n<link rel=\"preconnect\" href=\"https://fonts.googleapis.com\">\n<link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin>\n<link href=\"https://fonts.googleapis.com/css2?family=Inter:wght@400;600;800&display=swap\" rel=\"stylesheet\">\n\n<style>\n  .cl-includes {\n    --accent: #E41111;\n    --ink: #1a1a1a;\n    --muted: #444444;\n    --bg: #ffffff;\n    --card: #ffffff;\n    --radius: 18px;\n    --shadow: 0 8px 30px rgba(0,0,0,.06);\n    --border: #e5e7eb;\n    font-family: 'Inter', system-ui, -apple-system, Segoe UI, Roboto, Arial, sans-serif;\n    color: var(--ink);\n    background: var(--bg);\n  }\n\n  .cl-wrap {\n    max-width: 1200px;\n    margin: 0 auto;\n    padding: clamp(24px, 4vw, 48px);\n  }\n\n  .cl-card {\n    display: grid;\n    grid-template-columns: 1.05fr .95fr;\n    gap: clamp(20px, 4vw, 36px);\n    align-items: stretch;\n    background: var(--card);\n    border-radius: var(--radius);\n    box-shadow: var(--shadow);\n    border: 1px solid var(--border);\n    overflow: hidden;\n  }\n\n  .cl-copy h2 {\n    font-weight: 800;\n    font-size: clamp(26px, 3vw, 40px);\n    line-height: 1.2;\n    margin: 0 0 18px;\n    letter-spacing: .3px;\n    color: var(--ink);\n  }\n  .cl-subtle {\n    color: var(--muted);\n    font-size: 14px;\n    letter-spacing: .08em;\n    text-transform: uppercase;\n    margin-bottom: 10px;\n  }\n\n  .cl-list {\n    display: grid;\n    gap: 16px;\n    margin: 18px 0 26px;\n  }\n  .cl-item {\n    display: grid;\n    grid-template-columns: 40px 1fr;\n    gap: 12px;\n    align-items: start;\n  }\n  .cl-ico {\n    width: 40px; height: 40px;\n    border-radius: 10px;\n    background: #fff;\n    display: grid; place-items: center;\n    box-shadow: inset 0 0 0 1px rgba(0,0,0,.08);\n  }\n  .cl-ico svg { width: 22px; height: 22px; stroke: var(--accent); }\n  .cl-item h3 {\n    font-weight: 700;\n    font-size: 18px;\n    margin: 2px 0 4px;\n    color: var(--ink);\n  }\n  .cl-item p {\n    margin: 0;\n    color: var(--muted);\n    font-size: 16px;\n    line-height: 1.5;\n  }\n\n  .cl-cta {\n    display: inline-flex;\n    align-items: center;\n    gap: 10px;\n    background: var(--accent);\n    color: #fff;\n    padding: 14px 22px;\n    border-radius: 999px;\n    font-weight: 700;\n    font-size: 16px;\n    text-decoration: none;\n    box-shadow: 0 6px 18px rgba(228,17,17,.25);\n    transition: transform .05s ease, box-shadow .2s ease, background .2s ease;\n  }\n  .cl-cta:hover { transform: translateY(-1px); box-shadow: 0 8px 22px rgba(228,17,17,.3); }\n  .cl-cta:active { transform: translateY(0); }\n\n  .cl-media {\n    position: relative;\n    min-height: 320px;\n    background: #e9ecf1;\n    overflow: hidden;\n  }\n  .cl-media img {\n    position: absolute; inset: 0;\n    width: 100%; height: 100%;\n    object-fit: cover; object-position: center;\n  }\n\n  /* Mobile layout */\n  @media (max-width: 900px){\n    .cl-card { grid-template-columns: 1fr; }\n    /* text appears first, image second */\n    .cl-media { order: 2; }\n    .cl-copy { order: 1; }\n  }\n</style>\n\n<section class=\"cl-includes\" aria-label=\"What each Caffeine & Legends box includes\">\n  <div class=\"cl-wrap\">\n    <div class=\"cl-card\">\n      <!-- COPY -->\n      <div class=\"cl-copy\" style=\"padding: clamp(18px, 3.5vw, 36px);\">\n        <div class=\"cl-subtle\">Each Box Includes…</div>\n        <h2>Books, Brews & A Little Magic—Curated Monthly.</h2>\n\n        <div class=\"cl-list\">\n          <div class=\"cl-item\">\n            <div class=\"cl-ico\">\n              <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke-width=\"1.6\"><path d=\"M4 19.5A2.5 2.5 0 0 1 6.5 17H20\"/><path d=\"M20 22H6.5A2.5 2.5 0 0 1 4 19.5V4.6A2.6 2.6 0 0 1 6.6 2H20v20Z\"/></svg>\n            </div>\n            <div>\n              <h3>A Book</h3>\n              <p>A rotating mix of indie and well-known authors chosen to delight fantasy readers.</p>\n            </div>\n          </div>\n\n          <div class=\"cl-item\">\n            <div class=\"cl-ico\">\n              <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke-width=\"1.6\"><path d=\"M4 8h12v9a3 3 0 0 1-3 3H7a3 3 0 0 1-3-3V8Z\"/><path d=\"M16 10h2a3 3 0 1 1 0 6h-2\"/><path d=\"M9 4s-.5 1 .5 2M12 4s-.5 1 .5 2M6 4s-.5 1 .5 2\"/></svg>\n            </div>\n            <div>\n              <h3>A Drink</h3>\n              <p>Your choice of a delicious new coffee, tea, or drinking chocolate to try.</p>\n            </div>\n          </div>\n\n          <div class=\"cl-item\">\n            <div class=\"cl-ico\">\n              <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke-width=\"1.6\"><path d=\"M12 3.5 14.9 9l6.1.9-4.4 4.3 1 6.1L12 17.8 6.4 20.3l1-6.1L3 9.9 9.1 9 12 3.5Z\"/></svg>\n            </div>\n            <div>\n              <h3>A Special Find</h3>\n              <p>An extra item—usually from a small business—matched to the monthly theme.</p>\n            </div>\n          </div>\n\n          <div class=\"cl-item\">\n            <div class=\"cl-ico\">\n              <svg viewBox=\"0 0 24 24\" fill=\"none\" stroke-width=\"1.6\"><path d=\"M4 5a3 3 0 0 1 3-3h11v20H7a3 3 0 0 1-3-3V5Z\"/><path d=\"M7 6h8M7 10h8M7 14h6\"/></svg>\n            </div>\n            <div>\n              <h3>A Booklet</h3>\n              <p>A write-up about the curated items plus a themed recipe to enjoy.</p>\n            </div>\n          </div>\n        </div>\n\n        <a class=\"cl-cta\" href=\"https://legendarybox.com/#plan-selector\">\n          Subscribe Here\n          <svg aria-hidden=\"true\" width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M5 12h14M13 5l7 7-7 7\"/></svg>\n        </a>\n      </div>\n\n      <!-- MEDIA -->\n      <div class=\"cl-media\">\n        <img src=\"https://cdn.shopify.com/s/files/1/0305/4770/5900/files/ezgif-2-e502546d3d_1.jpg?v=1757103138\" alt=\"Curated bookshelf with fantasy reads and Caffeine & Legends box\" loading=\"lazy\" />\n      </div>\n    </div>\n  </div>\n</section>",
        "color_scheme": "scheme-2",
